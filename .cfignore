.next/cache/
cache/
*.tsbuildinfo

# Node modules (installed during build)
node_modules/

# Hardhat artifacts  
artifacts/
cache/

# Large development files
coverage/
.nyc_output/

# IDE files
.vscode/
.idea/

# OS files
.DS_Store
Thumbs.db

# Log files
*.log
logs/

# Git files (keep minimal git info)
.git/

# Environment files (handled by Cloudflare environment variables)
